{
  "manifest_version": 3,
  "name": "TSider_Fish",
  "description": "A simple sidebar of Yukigpt",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "sidebar.html"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
      }
  },
  "permissions": [
    "sidePanel",
    "tabs",
    "activeTab"
  ]
}
